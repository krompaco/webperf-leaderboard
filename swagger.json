{
  "openapi": "3.0.1",
  "info": {
    "title": "Webperf Core Leaderboard",
    "version": "v1"
  },
  "paths": {
    "/api/webperf-index": {
      "get": {
        "tags": [
          "Api"
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/WebperfIndexResponse"
                }
              }
            }
          },
          "500": {
            "description": "Server Error"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "WebperfTest": {
        "type": "object",
        "properties": {
          "testDate": {
            "type": "string",
            "format": "date-time"
          },
          "reportText": {
            "type": "string",
            "nullable": true
          },
          "reportJson": {
            "type": "string",
            "nullable": true
          },
          "rating": {
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      },
      "WebperfSite": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "nullable": true
          },
          "url": {
            "type": "string",
            "nullable": true
          },
          "rating": {
            "type": "number",
            "format": "double"
          },
          "tests": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/WebperfTest"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "WebperfIndexResponse": {
        "type": "object",
        "properties": {
          "sites": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/WebperfSite"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}